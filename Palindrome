#include<stdio.h>
#include<string.h>
int main()
{
    int i=0,len,flag;
    char str[10];
    printf("Enter a string:\n");
    scanf("%s",str);
    len=strlen(str);
    while(str[i]==str[len-i-1])
    {
        i++;
        flag=1;
    }
    if(flag==1)
    {
        printf("String is Palindrome");
    }
    else
    {
        printf("String is Not Palindrome");
    }
    return 0;
}
