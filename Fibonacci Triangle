#include<stdio.h>
void main()
{
    int i,j,n,a,b,c;
    printf("Enter the value of n:\n");
    scanf("%d",&n);
    for(i=0;i<=n;i++)
    {
        a=0;
        b=1;
        printf("%d\t",b);
        for(j=0;j<i;j++)
        {
            c=a+b;
            printf("%d\t",c);
            a=b;
            b=c;
         }
         printf("\n");
    }
}
